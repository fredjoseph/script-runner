{
  "name": "JSR",
  "version": "0.0.1",
  "description": "Simple Google Chrome extension to write and store javascript code which can be injected in active tab whenever you need it.",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "JSR",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "storage"
  ],
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}
